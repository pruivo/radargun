<config
        xmlns="urn:org:jgroups"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="urn:org:jgroups file://schema/JGroups-3.3.xsd">
    <UDP
            tos="8"
            ucast_recv_buf_size="20M"
            ucast_send_buf_size="640K"
            ip_mcast="false"
            ip_ttl="${jgroups.udp.ip_ttl:8}"
            mcast_recv_buf_size="25M"
            mcast_send_buf_size="640K"
            mcast_addr="${jgroups.udp.mcast_addr:232.10.10.10}"
            mcast_port="${jgroups.udp.mcast_port:45588}"
            loopback="false"
            discard_incompatible_packets="true"
            max_bundle_size="64K"
            enable_bundling="true"
            enable_unicast_bundling="true"
            enable_diagnostics="true"
            thread_naming_pattern="cl"
            thread_pool.enabled="true"
            thread_pool.min_threads="2"
            thread_pool.max_threads="32"
            thread_pool.keep_alive_time="30000"
            thread_pool.queue_enabled="true"
            thread_pool.queue_max_size="10"
            thread_pool.rejection_policy="discard"
            oob_thread_pool.enabled="true"
            oob_thread_pool.min_threads="4"
            oob_thread_pool.max_threads="64"
            oob_thread_pool.keep_alive_time="30000"
            oob_thread_pool.queue_enabled="true"
            oob_thread_pool.queue_max_size="10"
            oob_thread_pool.rejection_policy="discard"
            />
    <!--PING
            num_initial_members="5"
            break_on_coord_rsp="true"
            stagger_timeout="2000"
            timeout="5000"
            /-->
    <TCPGOSSIP
            break_on_coord_rsp="true"
            num_initial_members="10"
            timeout="5000"
            initial_hosts="149.165.148.106[12000]"
            />
    <MERGE2
            max_interval="30000"
            min_interval="10000"
            />
    <FD_SOCK/>
    <BARRIER/>
    <pbcast.NAKACK2
            use_mcast_xmit="false"
            use_mcast_xmit_req="false"
            discard_delivered_msgs="true"
            />
    <UNICAST3
            max_retransmit_time="0"
            conn_expiry_timeout="0"
            xmit_interval="1000"
            />
    <pbcast.STABLE
            stability_delay="2000"
            desired_avg_gossip="10000"
            max_bytes="1M"
            />
    <pbcast.GMS
            view_ack_collection_timeout="5000"
            print_local_addr="true"
            join_timeout="3000"
            max_bundling_time="500"
            view_bundling="true"
            />
    <SEQUENCER/>
    <tom.TOA/>
    <UFC
            max_credits="10M"
            min_threshold="0.4"
            />
    <MFC
            max_credits="10M"
            min_threshold="0.4"
            />
    <FRAG2
            frag_size="60K"
            />
</config>
